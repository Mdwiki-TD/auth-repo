parameters:
    level: 5
    treatPhpDocTypesAsCertain: false
    paths:
        - src/

    excludePaths:
        - *_old.php

    # Bootstrap vendor autoload for external library classes
    bootstrapFiles:
        - vendor/autoload.php

    # Scan files for class definitions
    scanFiles:
        - src/vendor_load.php
        - src/oauth/settings.php
